#!/bin/bash
cat_rosewater="#F5E0DC"
cat_flamingo="#F2CDCD"
cat_mauve="#DDB6F2"
cat_pink="#F5C2E7"
cat_maroon="#E8A2AF"
cat_red="#F28FAD"
cat_peach="#F8BD96"
cat_yellow="#FAE3B0"
cat_green="#ABE9B3"
cat_teal="#B5E8E0"
cat_blue="#96CDFB"
cat_sky="#89DCEB"
cat_lavender="#C9CBFF"
cat_black0="#161320"
cat_black1="#1A1826"
cat_black2="#1E1E2E"
cat_black3="#302D41"
cat_black4="#575268"
cat_gray0="#6E6C7E"
cat_gray1="#988BA2"
cat_gray2="#C3BAC6"
cat_white="#D9E0EE"

exec /usr/bin/bemenu -iH32 -mall --tb "$cat_black3" --tf "$cat_peach" --fb "$cat_black2" --ff "$cat_white" --nb "$cat_black2" --nf "$cat_white" --hb "$cat_peach" --hf "$cat_black0" --fn "Fira Code" "$@"
